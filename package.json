{
  "name": "livelywebpack2",
  "version": "1.0.0",
  "description": "webpack2",
  "author": "zaynex@126.com",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress",
